<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <record id="bp_seq_sale_order" model="ir.sequence">
        <field name="name">Sales Order</field>
        <field name="code">sale.order</field>
        <field name="prefix">TR-%(y)s%(month)s</field>
        <field name="padding">4</field>
        <field name="use_date_range">True</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="bp_seq_account_move_invoice" model="ir.sequence">
        <field name="name">Invoice Order</field>
        <field name="code">account.move.invoice</field>
        <field name="prefix">INV-F%(y)s%(month)s</field>
        <field name="padding">4</field>
        <field name="use_date_range">True</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="bp_seq_account_move_return_invoice" model="ir.sequence">
        <field name="name">Return Invoice Order</field>
        <field name="code">account.move.return.invoice</field>
        <field name="prefix">RINV-F%(y)s%(month)s</field>
        <field name="padding">4</field>
        <field name="use_date_range">True</field>
        <field name="company_id" eval="False"/>
    </record>

</odoo>
